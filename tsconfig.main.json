{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES2020",
    "lib": ["ES2020"],
    "moduleResolution": "node",
    "allowImportingTsExtensions": false,
    "outDir": "./dist",
    "noEmit": false,
    "jsx": "react"
  },
  "include": ["src/main/**/*.ts"],
  "exclude": ["node_modules", "dist"]
}

